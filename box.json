{
    "chmod": "0755",
    "directories": [
        ".git",
        "src"
    ],
    "finder": [
        {
            "name": [
                "*.php",
                "*.dist",
                "*.json"
            ],
            "exclude": [
                "tests",
                "Tests"
            ],
            "in": [
                "vendor"
            ]
        }
    ],
    "git-version": "package_version",
    "main": "bin/bengi",
    "output": "bengi.phar",
    "stub": true,
    "compactors": "Herrera\\Box\\Compactor\\Php",
    "compression": "GZ"
}